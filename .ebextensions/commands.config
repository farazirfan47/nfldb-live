container_commands:
  00-pre-req:
    command: "sudo yum install postgresql postgresql-devel python-devel"
  01-pyscopg:
    command: "sudo python -m pip install psycopg2"
  02-pytz:
    command: "sudo pip install pytz"
  03-enum:
    command: "sudo pip install pip enum34"
  04-httpd:
    command: "sudo pip install httplib2"
  05-bs4:
    command: "sudo pip install BeautifulSoup4"
    